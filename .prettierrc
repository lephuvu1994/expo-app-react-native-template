{
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "printWidth": 120,
    "trailingComma": "none",
    "endOfLine": "auto",
    "arrowParens": "always",
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
  
    "importOrder": [
      "^(react/(.*)$)|^(react$)|^(react-(.*)$)",
      "^(next/(.*)$)|^(next$)|^(next-(.*)$)",
      "^(@next)/(.*)$",
      "",
      "^(@radix-ui)/(.*)$",
      "^(@tanstack)/(.*)$",
      "^(zustand/(.*)$)|^(zustand$)",
      "<THIRD_PARTY_MODULES>",
      "",
      "^@/types",
      "^@/config",
      "^@/constants",
      "",
      "^@/providers",
      "^@/layouts",
      "",
      "^@/stores",
      "^@/schemas",
      "^@/services",
      "^@/hooks",
      "^@/lib",
      "^@/utils",
      "",
      "^@/components",
      "^@/shared",
      "^@/ui",
      "^@/widgets",
      "^@/skeletons",
      "",
      "^@/data",
      "^@/img",
      "",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true,
  
    "plugins": ["prettier-plugin-tailwindcss", "@ianvs/prettier-plugin-sort-imports"],
    "tailwindStylesheet": "./src/app/globals.css",
    "tailwindFunctions": ["clsx", "cva"]
  }
  